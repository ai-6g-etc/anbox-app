# Auto detect text files and perform LF normalization
* text=auto
*.img filter=lfs diff=lfs merge=lfs -text
node_exporter filter=lfs diff=lfs merge=lfs -text
perf_exporter filter=lfs diff=lfs merge=lfs -text
prometheus    filter=lfs diff=lfs merge=lfs -text
grafana-server filter=lfs diff=lfs merge=lfs -text
